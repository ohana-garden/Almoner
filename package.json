{
  "name": "almoner",
  "version": "0.1.2",
  "description": "A graph-native platform facilitating the flow of resources—grants, scholarships—to those who can use them, with impact made visible through contribution patterns.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/server.ts",
    "start": "node dist/server.js",
    "test": "jest",
    "test:connection": "ts-node scripts/test-connection.ts",
    "seed:phase1": "ts-node scripts/seed-phase1.ts",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "deploy:railway": "ts-node scripts/deploy-railway.ts"
  },
  "keywords": [
    "grants",
    "scholarships",
    "graph-database",
    "falkordb",
    "non-profit",
    "impact-tracking",
    "kala"
  ],
  "author": "Ohana Garden",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.0",
    "falkordb": "^6.4.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.19.27",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
